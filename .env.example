# 环境配置示例文件
# 复制为 .env 并填入实际值

# Cloudflare 配置
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token

# 数据库配置
D1_DATABASE_ID=your-d1-database-id
D1_DATABASE_ID_TEST=your-test-d1-database-id
D1_DATABASE_ID_PROD=your-prod-d1-database-id

# KV 存储配置
KV_NAMESPACE_ID=your-kv-namespace-id
KV_NAMESPACE_ID_TEST=your-test-kv-namespace-id
KV_NAMESPACE_ID_PROD=your-prod-kv-namespace-id

# R2 存储配置
R2_BUCKET_NAME=rmm-files
R2_BUCKET_NAME_TEST=rmm-files-test
R2_BUCKET_NAME_PROD=rmm-files-prod

# 服务端配置
API_BASE_URL=http://localhost:8787
ENROLLMENT_SECRET=your-enrollment-secret
JWT_SECRET=your-jwt-secret
WEBHOOK_SECRET=your-webhook-secret

# Agent 配置
AGENT_SERVER_URL=http://localhost:8787
AGENT_LOG_LEVEL=info
AGENT_CONFIG_PATH=./agent-config.json

# Console 配置
VITE_API_BASE_URL=http://localhost:8787